') ? JSON.parse(localStorage.getItem('user')) : null;\n\n    const [emotions, setEmotions] = useState([]);\n    const [isLoading, setIsLoading] = useState(false);\n\n    useEffect(() => {\n        setIsLoading(true);\n\n        notesService.getReport(userId, user.token)\n            .then(response => {\n                setEmotions(response);\n                setIsLoading(false);\n            })\n            .catch(err => {\n                console.log(err);\n            })\n    }, [userId]);\n\n    return (\n        <>  \n            {isLoading && <p>Loading...</p>}\n            {!isLoading && \n                <section className=\"flex flex-col items-center pb-10\">\n                    <h1 className=\"title mb-20\">Weekly report</h1>\n\n                    <ReportChart emotions={emotions}></ReportChart>\n                </section>\n            }   \n        \n        </>\n    )\n\n}\n\nexport default WeeklyReport;","/Users/alexandrinamehandzhiyska/Documents/alex/education/projects/da-day/DaDay/client/src/components/Register/Register.jsx",[],[],"/Users/alexandrinamehandzhiyska/Documents/alex/education/projects/da-day/DaDay/client/src/components/Notes/AllNotes/AllNotes.jsx",["105"],[],"import { useState, useEffect } from \"react\";\nimport {notesService} from '../../../services/notesService';\nimport { formatDate } from \"../../../utils\";\nimport { Link } from \"react-router-dom\";\n\nimport './AllNotes.css';\n\nconst AllNotes = () => {\n    const [notes, setNotes] = useState([]);\n    const user = localStorage.getItem('user') ? JSON.parse(localStorage.getItem('user')) : null;\n\n    useEffect(() => {\n        notesService.getReport(user.user_id, user.token)\n            .then((response) => {\n                setNotes(response.map(obj => { \n                    return { id: obj.id, description: obj.description, date: formatDate(obj.date)};\n                }));\n            })\n            .catch(err => {\n                console.log(err);\n            });\n    }, [user.user_id]);\n\n\n    return (\n        <section className=\"flex flex-wrap justify-around pt-5\">\n            {notes.map((note, i) => \n                <article className=\"note text-black bg-white rounded-xl transition duration-1000 ease-in-out w-80 h-full my-6 mx-6 p-5 break-words\" key={i}>\n\n                    <p>{note.description}</p>\n\n                    <div className=\"dateMade\">\n                        <p>Date: {note.date}</p>\n                    </div>\n\n                    <Link to={`/notes/${user.user_id}/${note.id}`} className=\"ml-4\">{'>'} Read note</Link>\n                </article>\n            )}\n        </section>\n    );\n}\n\nexport default AllNotes;","/Users/alexandrinamehandzhiyska/Documents/alex/education/projects/da-day/DaDay/client/src/components/Notes/NoteDetails/NoteDetails.jsx",["106"],[],"import { useEffect, useState } from \"react\";\nimport { useParams } from \"react-router\";\nimport { notesService } from \"../../../services/notesService\";\nimport { formatDate } from \"../../../utils\";\nimport { Link } from \"react-router-dom\";\n\nconst NoteDetails = () => {\n    const [note, setNote] = useState(null);\n    const [isLoading, setIsLoading] = useState(true);\n    const { userId, noteId } = useParams();\n\n    const user = localStorage.getItem('user') ? JSON.parse(localStorage.getItem('user')) : null;\n\n    useEffect(() => {\n        notesService.getNote(noteId, user.token)\n            .then(response => {\n                const note = response.note;\n                note.date = formatDate(note.date);\n\n                setNote(note);\n                setIsLoading(false);\n            })\n            .catch(err => {\n                console.log(err);\n            });\n    }, [noteId]);\n    \n    return (\n        <section className=\"flex justify-center pt-10\">\n            {isLoading && <p>Loading...</p>}\n            {!isLoading && \n                <article className=\"note text-black bg-white rounded-xl transition duration-1000 ease-in-out w-1/2 my-6 mx-6 break-words p-5\">\n                    <p>{note.description}</p>\n\n                    <div className=\"dateMade\">\n                        <p>Date: {note.date}</p>\n                    </div>\n\n                    <Link to={`/notes/${userId}`} className=\"ml-4\">{'>'} Go Back</Link>\n                </article>\n            }\n        </section>\n       \n    );\n}\n\nexport default NoteDetails;","/Users/alexandrinamehandzhiyska/Documents/alex/education/projects/da-day/DaDay/client/src/components/Notes/CreateNote/CreateNote.jsx",["107"],[],"import { useEffect } from \"react\";\nimport { useState } from \"react\";\nimport { notesService } from \"../../../services/notesService\";\nimport { useForm } from \"react-hook-form\";\n\nimport { avatarData } from '../../../avatarData';\n\nimport './CreateNote.css';\n\nconst CreateNote = () => {\n    const [question, setQuestion] = useState(null);\n    const [isLoading, setIsLoading] = useState(false);\n    const [emotion, setEmotion] = useState(null);\n    const [isSubmitted, setIsSubmitted] = useState(false);\n\n    const user = localStorage.getItem('user') ? JSON.parse(localStorage.getItem('user')) : null;\n\n    const { register, handleSubmit } = useForm({ mode: 'onSubmit', reValidateMode: 'onChange' });\n\n    const emotions = avatarData[user.avatar - 1];\n\n    useEffect(() => {\n        setIsLoading(true);\n\n        notesService.getOne(user)\n            .then(response => {\n                const question = response.question.replace(/<.+>/, user.username);\n                setQuestion(question);\n                setIsLoading(false);\n            })\n            .catch(err => {\n                console.log(err);\n            })\n    }, [user.user_id, user.username]);\n\n    const sendAnswer = (data) => {\n        notesService.createOne(user, { avatar: emotion, text: data.description })\n            .then(() => {\n                setIsSubmitted(true);\n            })\n            .catch(err => {\n                console.log(err);\n            });\n    } \n\n    return (\n        <section className=\"flex flex-col items-center justify-center\">\n            {isLoading && <p>Loading ...</p>}\n            {!isLoading && !emotion &&\n                <section className=\"flex px-10\">\n                    <article className=\"animated-img mt-32\">\n                        <img src={emotions[1]} alt=\"Avatar\" className=\"animated-avatar\"/>\n                    </article>\n\n                    <article className=\"emotions flex flex-col items-center mt-28\">\n                        <h1 className=\"text-5xl my-10 text-center\">{question}</h1>\n                        <section className=\"flex flex-wrap mt-10\">\n                            {emotions.slice(2).map((emotion, i) => <img key={i} onClick={() => setEmotion(i + 1)} src={emotion} alt=\"avatar\" className=\"emotion-card w-28 h-28 mx-3 my-5 rounded-3xl\"/>)}\n                        </section>\n                    </article>\n                </section>\n            }\n\n            {!isLoading && emotion && !isSubmitted &&\n                <section className=\"flex px-10\">\n                    <article className=\"animated-img mt-32\">\n                        <img src={emotions[1]} alt=\"Avatar\" className=\"animated-avatar\"/>\n                    </article>\n\n                    <article className=\"answer flex flex-col items-center mt-28\">\n                        <h1 className=\"text-5xl my-10 text-center\">Why do you feel this way?</h1>\n                        \n                        <form onSubmit={handleSubmit(sendAnswer)} className=\"flex flex-col\">\n                            <textarea {...register('description', { required: { value: true } })} name=\"description\" type=\"text\" className=\"desc mt-20 bg-transparent border-2 border-orange-200\" />\n                            <input type=\"submit\" value=\"Send Answer\" />\n                        </form>\n                    </article>\n                </section>\n            }\n\n            {!isLoading && emotion && isSubmitted &&\n                <section className=\"flex px-10\">\n                    <article className=\"animated-img mt-32\">\n                        <img src={emotions[1]} alt=\"Avatar\" loop=\"infinite\" className=\"animated-avatar\"/>\n                    </article>\n\n                    <article>\n                        <h1 className=\"title mt-36\">Thank you for your response! I'd be glad to chat with you tomorrow too!</h1>\n                    </article>\n                </section>\n            }\n        </section>\n    )\n}\n\nexport default CreateNote;","/Users/alexandrinamehandzhiyska/Documents/alex/education/projects/da-day/DaDay/client/src/utils.js",[],[],"/Users/alexandrinamehandzhiyska/Documents/alex/education/projects/da-day/DaDay/client/src/avatarData.js",[],[],"/Users/alexandrinamehandzhiyska/Documents/alex/education/projects/da-day/DaDay/client/src/services/authService.js",[],[],"/Users/alexandrinamehandzhiyska/Documents/alex/education/projects/da-day/DaDay/client/src/services/notesService.js",[],[],"/Users/alexandrinamehandzhiyska/Documents/alex/education/projects/da-day/DaDay/client/src/components/WeeklyReport/ReportChart.jsx",[],[],"/Users/alexandrinamehandzhiyska/Documents/alex/education/projects/da-day/DaDay/client/src/components/Header/Header.jsx",[],[],"/Users/alexandrinamehandzhiyska/Documents/alex/education/projects/da-day/DaDay/client/src/components/Footer/Footer.jsx",["108","109","110"],[],"/Users/alexandrinamehandzhiyska/Documents/alex/education/projects/da-day/DaDay/client/src/constants.js",[],[],"/Users/alexandrinamehandzhiyska/Documents/alex/education/projects/da-day/DaDay/client/src/components/common/GuardRoute.jsx",[],[],{"ruleId":"111","replacedBy":"112"},{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","severity":1,"message":"116","line":25,"column":8,"nodeType":"117","endLine":25,"endColumn":16,"suggestions":"118